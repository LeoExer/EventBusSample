<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Goto SubscriberExceptionsActivity"
        android:onClick="startSubscriberExceptionsActivity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Goto NoSubscriberActivity"
        android:onClick="startNoSubscriberActivity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Goto EventInheritanceActivity"
        android:onClick="startEventInheritanceActivity"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Goto IgnoreMethodActivity"
        android:onClick="startIgnoreMethodActivity"/>

</LinearLayout>
